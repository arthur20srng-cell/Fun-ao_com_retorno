#include <iostream>
using namespace std;

    int maiorElemento vet[], int tamanho) {
        int maior = vet[0];
        
        for(int i = 1; i < tamanho; i++) {
            if(vet[i] > maior) {
                maior = vet[i];
            }
        }
        
        return maior;
    }
    
int main()
{
    int n;
    
    cout << "Digite o tamanho do vetor: ";
    cin >> n;
    
    int vetor[n];
    
    cout << "Digite os elementos do vetor:" << endl;
    for(int i = 0; i < n; i++) {
        cin >> vetor[i];
    }
    cout << "O maior elemento e: " << maiorelemento (vetor, n) << end;
    
    return 0;
}
